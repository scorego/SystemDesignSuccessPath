  "bugs": {
    "url": "https://github.com/scorego/SystemDesignSuccessPath/issues"
  },
  "homepage": "https://not-available-now.com/",
  "devDependencies": {
    "gitbook-cli": "^2.3.2"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "honkit": "^6.0.3",
    "js-yaml": "^4.1.0"
  }
